<?xml version='1.0' encoding='UTF-8'?>
<webservices xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
  <webservice-description>
    <webservice-description-name>com.bea.medrec.webservices.MedRecWebServices</webservice-description-name>
    <wsdl-file>WEB-INF/MedRecWebServices.wsdl</wsdl-file>
    <jaxrpc-mapping-file>WEB-INF/MedRecWebServices.xml</jaxrpc-mapping-file>
    <port-component>
      <port-component-name>MedRecWebServicesPort</port-component-name>
      <wsdl-port xmlns:med="http://www.bea.com/medrec">med:MedRecWebServicesPort</wsdl-port>
      <service-endpoint-interface>com.bea.medrec.webservices.MedRecWebServicesPortType</service-endpoint-interface>
      <service-impl-bean>
        <servlet-link>MedRecWebServicesServlethttp</servlet-link>
      </service-impl-bean>
    </port-component>
  </webservice-description>
</webservices>
